class Solution {
public:
    int searchInsert(vector<int>& nums, int target) {
        int n=nums.size();
        int mid=0;
        int st=0, en=n-1;
         while(st<=en){
          mid=st+(en-st)/2;
        
            if(nums[mid]==target)return mid;
          else if(nums[mid]<target)st=mid+1;
          else{
            en=mid-1;
          
         }
         }
         return st;
         
    }
};
