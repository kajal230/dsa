class Solution {
public:
    ListNode* deleteDuplicates(ListNode* head) {
        if(head==nullptr)return head;
        ListNode* temp1=head;
        
        while(temp1->next!=nullptr){
            if(temp1->val==temp1->next->val){
                temp1->next=temp1->next->next;
            }else{
           temp1=temp1->next;
            }
        }
        return head;
    }
};
